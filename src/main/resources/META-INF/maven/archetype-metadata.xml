<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="ImageJ 1.x plugin"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <requiredProperties>
    <requiredProperty key="name">
      <defaultValue>N/A</defaultValue>
    </requiredProperty>
    <requiredProperty key="description">
      <defaultValue>N/A</defaultValue>
    </requiredProperty>

    <requiredProperty key="developerID">
      <defaultValue>N/A</defaultValue>
    </requiredProperty>
    <requiredProperty key="developerName">
      <defaultValue>N/A</defaultValue>
    </requiredProperty>
    <requiredProperty key="developerEmail">
      <defaultValue>N/A</defaultValue>
    </requiredProperty>
    <requiredProperty key="developerURL">
      <defaultValue>N/A</defaultValue>
    </requiredProperty>
    <requiredProperty key="developerTimezone">
      <defaultValue>N/A</defaultValue>
    </requiredProperty>

    <requiredProperty key="organizationName">
      <defaultValue>N/A</defaultValue>
    </requiredProperty>
    <requiredProperty key="organizationURL">
      <defaultValue>N/A</defaultValue>
    </requiredProperty>

    <requiredProperty key="githubRepository">
      <defaultValue>N/A</defaultValue>
    </requiredProperty>
  </requiredProperties>

  <fileSets>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/main/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/main/resources</directory>
      <includes>
        <include>**/*.config</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>.gitignore</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>
